	<queryString language="SQL">
		<![CDATA[SELECT
	c.numero_cte,
	c.cte_nombre,
	a.aviso_cve,
	s.servicio_ds,
	u.unidad_de_manejo_ds,
	ps.precio,
	a.aviso_cve,
    a.aviso_vigencia,
    ca.categoria_ds
FROM cliente c
INNER JOIN precio_servicio ps ON c.cte_cve = ps.cliente
INNER JOIN servicio s ON s.servicio_cve = ps.servicio
INNER JOIN unidad_de_manejo u ON ps.unidad = u.unidad_de_manejo_cve
LEFT JOIN aviso a ON ps.aviso_cve = a.aviso_cve
LEFT JOIN categoria ca on ca.categoria_cve = a.categoria_cve
WHERE (c.cte_cve = $P{idCliente} OR $P{idCliente} IS NULL)
ORDER BY
	c.cte_nombre ASC,
	a.aviso_cve ASC,
	s.servicio_ds ASC]]>
